{
  "name": "test/project",
  "type": "project",
  "require": {
    "rcknr/composer-auth-dotenv": "*",
    "directorytree/dummy": "^1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "{TEST_URL}"
    },
    {
      "type": "path",
      "url": "repository/"
    }
  ],
  "config": {
    "secure-http": false,
    "allow-plugins": {
      "rcknr/composer-auth-dotenv": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
